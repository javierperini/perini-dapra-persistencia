<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="unq.tpi.persistencia.Asiento" table="ASIENTO" >
        <id name="id" type="java.lang.Integer">
            <column name="S_ID" />
            <generator class="identity" />
        </id>

        <property name="precioPorCategoria" type="string">
            <column name="S_CAT_PRECIO" length="20" not-null="true" unique="false" />
        </property>

    </class>
</hibernate-mapping>