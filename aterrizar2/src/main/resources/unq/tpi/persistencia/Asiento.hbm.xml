<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="unq.tpi.persistencia.Asiento" table="Asiento" >
        <id name="id" type="java.lang.Integer">
            <column name="idVuelo" />
            <generator class="identity" />
        </id>
        <property name="nro" type="java.lang.Integer">
              <column name="nroAsiento" length="20" not-null="true" unique="false" />
        </property>

        <property name="precioPorCategoria" type="java.lang.Integer">
            <column name="precioDeCategoria" length="20" not-null="true" unique="false" />
        </property>
        
       <many-to-one name="tramo" column="idTramo"  />
       <many-to-one name="reservadoPor" column="idUsuario"  />
      
      
    </class>
</hibernate-mapping>